{
  "nav-group": {
    "font": {
      "value": {
        "fontFamily": "{fontFamilies.body}",
        "fontWeight": "{fontWeights.medium}",
        "lineHeight": "{lineHeights.textSmallMobile}",
        "fontSize": "{fontSizes.textSmallMobile}"
      },
      "type": "typography"
    },
    "mobile": {
      "background": {
        "value": "{bg.white.value}",
        "type": "color"
      },
      "list": {
        "margin": {
          "value": "{spacing.175.value} 0 0",
          "type": "spacing"
        }
      },
      "padding": {
        "value": "{spacing.100.value} {spacing.175.value} {spacing.950.value} {spacing.175.value}",
        "type": "spacing"
      },
      "trigger": {
        "border": {
          "radius": {
            "value": "{border.radius.md.value}",
            "type": "other"
          },
          "width": {
            "value": "{border.width.md.value}",
            "type": "borderWidth"
          }
        },
        "margin": {
          "value": "{spacing.100.value}",
          "type": "spacing"
        },
        "text": {
          "value": "{color.blue-muted.value}",
          "type": "color"
        }
      }
    },
    "sideNav": {
      "dropdown": {
        "padding": {
          "value": "{spacing.350.value}",
          "type": "spacing"
        }
      },
      "trigger": {
        "fontWeight": {
          "value": "{fontWeights.semibold.value}",
          "type": "color"
        },
        "hover": {
          "background": {
            "value": "{bg.light.value}",
            "type": "color"
          }
        },
        "icon": {
          "margin": {
            "value": "{spacing.100.value}",
            "type": "spacing"
          }
        },
        "margin": {
          "value": "{spacing.100.value}",
          "type": "spacing"
        }
      }
    },
    "topNav": {
      "dropdown": {
        "background": {
          "value": "{bg.white.value}",
          "type": "color"
        },
        "box-shadow": {
          "value": "0 0 {spacing.100.value} rgba(0, 0, 0, 0.25)",
          "type": "color"
        },
        "padding": {
          "value": "{spacing.150.value} {spacing.150.value} {spacing.25.value}",
          "type": "spacing"
        },
        "width": {
          "value": "{container.xs.value}",
          "type": "sizing"
        }
      },
      "trigger": {
        "hover": {
          "background": {
            "value": "{color.grayscale.150.value}",
            "type": "color"
          },
          "text": {
            "value": "{link.hover.value}",
            "type": "color"
          },
          "decoration": {
            "thickness": {
              "value": "{border.width.md.value}",
              "type": "borderWidth"
            }
          }
        },
        "decoration": {
          "thickness": {
            "value": "{border.width.sm.value}",
            "type": "borderWidth"
          }
        },
        "underline": {
          "offset": {
            "value": "{spacing.50.value}",
            "type": "borderWidth"
          }
        },
        "expanded": {
          "background-color": {
            "value": "{color.grayscale.150.value}",
            "type": "color"
          }
        },
        "icon": {
          "margin": {
            "value": "{spacing.100.value}",
            "type": "spacing"
          }
        },
        "padding": {
          "value": "{spacing.225.value} {spacing.200.value}",
          "type": "spacing"
        }
      }
    },
    "trigger": {
      "focus": {
        "background": {
          "value": "{focus.background.value}",
          "type": "color"
        },
        "text": {
          "value": "{focus.text.value}",
          "type": "color"
        },
        "border-radius": {
          "value": "{border.radius.sm.value}",
          "type": "other"
        },
        "outline-offset": {
          "value": "{border.width.md.value}",
          "type": "borderWidth"
        },
        "outline": {
          "value": "{outline.width.value} solid {focus.background.value}",
          "type": "other"
        },
        "box-shadow": {
          "value": "0 0 0 {spacing.25.value} {color.white.value}",
          "type": "color"
        }
      },
      "hover": {
        "text": {
          "value": "{link.hover.value}",
          "type": "color"
        }
      },
      "maxWidth": {
        "value": "{container.xs.value}",
        "type": "sizing"
      },
      "padding": {
        "value": "{spacing.125.value} {spacing.150.value}",
        "type": "spacing"
      },
      "text": {
        "value": "{text.primary.value}",
        "type": "color"
      }
    }
  }
}
