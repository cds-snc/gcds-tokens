{
  "notice": {
    "border": {
      "width": {
        "value": "{border.width.xl.value}",
        "type": "borderWidth"
      }
    },
    "content": {
      "heading": {
        "font": {
          "desktop": {
            "value": {
              "fontFamily": "{fontFamilies.heading}",
              "fontWeight": "{fontWeights.bold}",
              "lineHeight": "{lineHeights.h3}",
              "fontSize": "{fontSizes.h3}"
            },
            "type": "typography"
          },
          "mobile": {
            "value": {
              "fontFamily": "{fontFamilies.heading}",
              "fontWeight": "{fontWeights.bold}",
              "lineHeight": "{lineHeights.h3Mobile}",
              "fontSize": "{fontSizes.h3Mobile}"
            },
            "type": "typography"
          }
        },
        "marginBlockStart": {
          "desktop": {
            "value": "{spacing.200.value}",
            "type": "spacing"
          },
          "mobile": {
            "value": "{spacing.250.value}",
            "type": "spacing"
          }
        }
      },
      "slotted": {
        "font": {
          "desktop": {
            "value": {
              "fontFamily": "{fontFamilies.body}",
              "fontWeight": "{fontWeights.regular}",
              "lineHeight": "{lineHeights.text}",
              "fontSize": "{fontSizes.text}"
            },
            "type": "typography"
          },
          "mobile": {
            "value": {
              "fontFamily": "{fontFamilies.body}",
              "fontWeight": "{fontWeights.regular}",
              "lineHeight": "{lineHeights.textMobile}",
              "fontSize": "{fontSizes.textMobile}"
            },
            "type": "typography"
          }
        },
        "list": {
          "margin": {
            "value": "{spacing.300.value}",
            "type": "spacing"
          }
        },
        "margin": {
          "value": "{spacing.100.value}",
          "type": "spacing"
        }
      }
    },
    "danger": {
      "text": {
        "value": "{danger.text.value}",
        "type": "color"
      }
    },
    "icon": {
      "afterHeight": {
        "value": "calc(100% - ({spacing.600.value} + {spacing.100.value}))",
        "type": "sizing"
      },
      "beforeHeight": {
        "value": "{spacing.225.value}",
        "type": "sizing"
      },
      "gap": {
        "value": "{spacing.175.value}",
        "type": "spacing"
      },
      "margin": {
        "value": "{spacing.25.value} auto 0",
        "type": "spacing"
      },
      "width": {
        "value": "{spacing.450.value}",
        "type": "sizing"
      }
    },
    "info": {
      "text": {
        "value": "{color.blue.750.value}",
        "type": "color"
      }
    },
    "text": {
      "value": "{text.primary.value}",
      "type": "color"
    },
    "success": {
      "text": {
        "value": "{color.green.700.value}",
        "type": "color"
      }
    },
    "warning": {
      "text": {
        "value": "{color.yellow.500.value}",
        "type": "color"
      }
    }
  }
}
